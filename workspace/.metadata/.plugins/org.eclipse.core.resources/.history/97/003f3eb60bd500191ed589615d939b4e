scchart pacemaker {
  input signal ASense, VSense;
  output signal APace, VPace;

  initial state main { /*AVI – Atrioventricular Interval
The maximum time between an atrial event and its subsequent ventricular event. */

    region AVI:

    initial state idle;

    region PVARP:

    initial state idle;

    region VRP:

    initial state idle;

    region AEI:

    initial state idle;

    region LRI:

    initial state idle;

    region URI:

    initial state idle;
  };
}